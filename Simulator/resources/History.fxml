<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HistoryController">
   <children>
      <Label contentDisplay="CENTER" layoutX="262.0" layoutY="14.0" text="History" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="fetchButton" layoutX="25.0" layoutY="57.0" mnemonicParsing="false" text="Fetch" />
      <Button fx:id="optimalButton" layoutX="26.0" layoutY="91.0" mnemonicParsing="false" text="optimal" />
      <Button fx:id="averageButton" layoutX="25.0" layoutY="126.0" mnemonicParsing="false" text="average" />
      <LineChart fx:id="historyChart" layoutX="100.0" layoutY="50.0" prefHeight="350.0" prefWidth="500.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
